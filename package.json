{
  "name": "zyrotec-electron",
  "version": "0.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p electron:run ng:serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron:run": "wait-on tcp:4200 && tsc -p tsconfig.electron.json && electron . --serve",
    "ng:serve": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "electron-browser-storage": "^1.0.7",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "mica-electron-ts": "^1.2.8",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "wait-on": "^7.0.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "electron": "^22.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2"
  }
